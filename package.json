    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "better-docs": "^2.3.2",
    "date-fns": "^2.25.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.7",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
